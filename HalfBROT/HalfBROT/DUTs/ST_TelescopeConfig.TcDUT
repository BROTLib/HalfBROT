<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4026.8">
  <DUT Name="ST_TelescopeConfig" Id="{13de919a-2bce-4e8d-abc6-4c39522fc874}">
    <Declaration><![CDATA[TYPE ST_TelescopeConfig :
STRUCT
	// location
	longitude			: LREAL; // telescope site longitude
	latitude			: LREAL; // telescope site latitude
	altitude			: LREAL; // telescope site height above sea level: estimated
	
	// park position
	elevationPark		: LREAL := 60.0;
	azimuthPark			: LREAL := 359.999;
	derotatorPark		: LREAL := 229.85;

	// home position
	elevationHome		: LREAL := 55.0;
	azimuthHome			: LREAL := 180.0;
	derotatorHome		: LREAL := 229.85;
END_STRUCT
END_TYPE
]]></Declaration>
  </DUT>
</TcPlcObject>